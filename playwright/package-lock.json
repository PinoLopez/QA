{
  "name": "playwright", 
  "version": "1.0.0", 
  "lockfileVersion": 3, // Versión del formato del lockfile
  "requires": true, // Indica si el lockfile requiere que se instalen dependencias.
  "packages": 
  { // Detalles de todos los paquetes instalados.
    "": { // Información del paquete raíz (el proyecto principal).
      "name": "playwright", // Nombre del paquete raíz.
      "version": "1.0.0", 
      "license": "ISC", 
      "devDependencies": 
      { 
        "@types/node": "^22.13.10", // Tipos de Node.js para TypeScript.
        "playwright": "^1.51.0", // Framework de testing 
        "typescript": "^5.8.2" // Compilador de TypeScript.
      }
    },
    "node_modules/@types/node": 
    { // Detalles del paquete @types/node.
      "version": "22.13.10",
      "resolved": "...", // URL del paquete en el registro npm.
      "integrity": "...", // Hash de integridad del paquete.
      "dev": true, // Indica que es una dependencia de desarrollo.
      "license": "MIT",
      "dependencies": 
      { 
        "undici-types": "~6.20.0" // para undici.
      }
    },
    "node_modules/fsevents": 
    { // Detalles del paquete fsevents.
      "version": "2.3.2", 
      "resolved": "...", // URL del paquete en el registro npm.
      "integrity": "...", // Hash de integridad del paquete.
      "dev": true, // Dependencia de desarrollo.
      "hasInstallScript": true, // Tiene un script de instalación.
      "license": "MIT", 
      "optional": true, // Dependencia opcional.
      "os": 
      [ // Dependencia específica para macOS.
        "darwin"
      ],
      "engines": 
      { // Versiones de Node.js soportadas.
        "node": "^8.16.0 || ^10.6.0 || >=11.0.0"
      }
    },
    "node_modules/playwright": 
    { // Detalles del paquete playwright.
      "version": "1.51.0", 
      "resolved": "...", // URL del paquete en el registro npm.
      "integrity": "...", // Hash de integridad del paquete.
      "dev": true, // Dependencia de desarrollo.
      "license": "Apache-2.0", 
      "dependencies": 
      { 
        "playwright-core": "1.51.0" // Núcleo de Playwright.
      },
      "bin": 
      { // Ejecutables del paquete.
        "playwright": "cli.js"
      },
      "engines": 
      { // Versiones de Node.js soportadas.
        "node": ">=18"
      },
      "optionalDependencies": 
      { // Dependencias opcionales.
        "fsevents": "2.3.2"
      }
    },
    "node_modules/playwright-core": 
    { // Detalles del paquete playwright-core.
      "version": "1.51.0", 
      "resolved": "...", // URL del paquete en el registro npm.
      "integrity": "...", // Hash de integridad del paquete.
      "dev": true, // Dependencia de desarrollo.
      "license": "Apache-2.0", 
      "bin": 
      { // Ejecutables del paquete.
        "playwright-core": "cli.js"
      },
      "engines": 
      { // Versiones de Node.js soportadas.
        "node": ">=18"
      }
    },
    "node_modules/typescript": { // Detalles del paquete typescript.
      "version": "5.8.2", // Versión del paquete.
      "resolved": "...", // URL del paquete en el registro npm.
      "integrity": "...", // Hash de integridad del paquete.
      "dev": true, // Dependencia de desarrollo.
      "license": "Apache-2.0", // Licencia del paquete.
      "bin": { // Ejecutables del paquete.
        "tsc": "bin/tsc",
        "tsserver": "bin/tsserver"
      },
      "engines": { // Versiones de Node.js soportadas.
        "node": ">=14.17"
      }
    },
    "node_modules/undici-types": 
    { // Detalles del paquete undici-types.
      "version": "6.20.0", 
      "resolved": "...", // URL del paquete en el registro npm.
      "integrity": "...", // Hash de integridad del paquete.
      "dev": true, // Dependencia de desarrollo.
      "license": "MIT" 
    }
  }
}